{
  "kind": "collectionType",
  "collectionName": "service_categories",
  "info": {
    "singularName": "service-category",
    "pluralName": "service-categories",
    "displayName": "Service Category",
    "description": "Categories for organizing services"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 50
    },
    "description": {
      "type": "text"
    },
    "color": {
      "type": "string",
      "default": "#6366f1"
    },
    "icon": {
      "type": "string"
    },
    "displayOrder": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "isDefault": {
      "type": "boolean",
      "default": false
    },
    "parentCategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service-category.service-category",
      "inversedBy": "subcategories"
    },
    "subcategories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service-category.service-category",
      "mappedBy": "parentCategory"
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service",
      "mappedBy": "category"
    },
    "tenant": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tenant.tenant",
      "required": true
    }
  }
}