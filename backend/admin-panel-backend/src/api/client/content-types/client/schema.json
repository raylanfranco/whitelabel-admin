{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "isOnline": {
      "type": "boolean"
    },
    "lastSeen": {
      "type": "datetime"
    },
    "notes": {
      "type": "text"
    },
    "referralSource": {
      "type": "string"
    },
    "clientStatus": {
      "type": "enumeration",
      "enum": [
        "active",
        "inactive",
        "archived"
      ]
    },
    "conversation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::conversation.conversation",
      "mappedBy": "client"
    },
    "leadViewedAt": {
      "type": "datetime"
    }
  }
}
